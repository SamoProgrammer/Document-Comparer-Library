<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
  </PropertyGroup>

  <PropertyGroup>
    <PackageId>DocumentComparer</PackageId>
    <Version>1.0.0</Version>
    <Authors>Mohammad Mohammadi (SamoProgrammer)</Authors>
    <Description>A simple and fast dotnet library for comparing documents and detecting differences.</Description>
   <PackageTags>document, comparison, diff, document comparer</PackageTags>
  </PropertyGroup>


  <ItemGroup>
    <PackageReference Include="DocumentFormat.OpenXml" Version="3.1.1" />
    <PackageReference Include="itext7" Version="8.0.5" />
  </ItemGroup>

</Project>
